<h1>Articles</h1>
<%= link_to "New Article", new_article_path %>
<ul>
  <% @articles.each do |article| %>
   <% unless article.archived? %>
    <li>
      <%= article.title %>
      <%= link_to 'Show', article %>
      <%= link_to "Edit", edit_article_path(article) %>
      <%= link_to "Destroy", article_path(article) %>
    </li>
  <% end %>
  <% end %>
</ul>
